<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="409" id="com.mediabeef.mwcog" version="1.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Commuter Connections</name>
    <description>
        Commuter Connections Mobile
    </description>
    <author email="mike@mediabeef.com" href="http://mediabeef.com">
        Media Beef
    </author>

    <content src="index.html" />
    <preference name="phonegap-version" value="cli-6.5.0" />
    <preference name="websecurity" value="disable" />
    <preference name="webviewbounce" value="false" />
    <icon src="www/icon.png" />
    <icon gap:platform="ios" height="120" src="www/res/icon/ios/icon-120.png" width="120" />
    <icon gap:platform="ios" height="152" src="www/res/icon/ios/icon-152.png" width="152" />
    <icon gap:platform="ios" height="76" src="www/res/icon/ios/icon-76.png" width="76" />
    <icon gap:platform="ios" height="57" src="www/res/icon/ios/icon-57.png" width="57" />
    <icon gap:platform="ios" height="72" src="www/res/icon/ios/icon-72.png" width="72" />
    <icon gap:platform="ios" height="114" src="www/res/icon/ios/icon-57-2x.png" width="114" />
    <icon gap:platform="ios" height="144" src="www/res/icon/ios/icon-72-2x.png" width="144" />
    <icon gap:platform="blackberry" src="www/res/icon/blackberry/icon-80.png" />
    <icon gap:platform="blackberry" gap:state="hover" src="www/res/icon/blackberry/icon-80.png" />
    <icon gap:density="ldpi" gap:platform="android" src="www/res/icon/android/icon-36-ldpi.png" />
    <icon gap:density="mdpi" gap:platform="android" src="www/res/icon/android/icon-48-mdpi.png" />
    <icon gap:density="hdpi" gap:platform="android" src="www/res/icon/android/icon-72-hdpi.png" />
    <icon gap:density="xhdpi" gap:platform="android" src="www/res/icon/android/icon-96-xhdpi.png" />
    <icon gap:density="xxhdpi" gap:platform="android" src="www/res/icon/android/icon-144-xxhdpi.png" />
    <icon gap:density="xxxhdpi" gap:platform="android" src="www/res/icon/android/icon-192-xxxhdpi.png" />
    <gap:splash gap:platform="ios" height="480" src="www/res/screen/ios/screen-iphone.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="www/res/screen/ios/screen-iphone-portrait-2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1136" src="www/res/screen/ios/screen-iphone-portrait-568h-2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1004" src="www/res/screen/ios/screen-ipad-portrait.png" width="768" />
    <gap:splash gap:platform="ios" height="2008" src="www/res/screen/ios/screen-ipad-portrait-2x.png" width="1536" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.0" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.5.0" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.0" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.0" />
    <plugin name="cordova-plugin-x-toast" spec="~2.5.2" />
    <access origin="*" />
    <access origin="https://*.commuterconnections.org" />
    <access origin="http://*.mediabeef.com" />
    <access origin="https://*.mediabeef.com" />
    <access origin="https://*.google-analytics.com" />
    <access origin="http://*.google.com" />
    <access origin="http://*.googleapis.com" />
    <access origin="http://*.gstatic.com" />
    <access origin="https://*.google.com" />
    <access origin="https://*.googleapis.com" />
    <access origin="https://*.gstatic.com" />
    <access origin="https://maps.googleapis.com" />
    <allow-navigation href="http://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <config-file mode="delete" parent="/manifest" platform="android">
        <uses-permission name="android.permission.WRITE_EXTERNAL_STORAGE" />
        <uses-permission name="android.permission.READ_EXTERNAL_STORAGE" />
    </config-file>
    <plugin name="cordova-plugin-statusbar" spec="~2.2.1" />
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <plugin name="cordova-plugin-screen-orientation" spec="~1.4.2" />
    <preference name="fullscreen" value="false" />
    <item name="android:windowFullscreen">false</item>
    <preference name="android-windowSoftInputMode" value="stateVisible|adjustResize" />
    <plugin name="cordova-plugin-device" spec="~1.1.5" />
</widget>
